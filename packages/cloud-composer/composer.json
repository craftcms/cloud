{
  "name": "craftcms/cloud-composer",
  "description": "Composer plugin for Craft Cloud autoload management",
  "type": "composer-plugin",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "craft\\cloud\\composer\\": "src/"
    }
  },
  "extra": {
    "class": "craft\\cloud\\composer\\ComposerPlugin",
    "plugin-optional": true,
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  }
}
